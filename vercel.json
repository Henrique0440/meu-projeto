{
  "version": 2,
  "builds": [
    {
      "src": "pages/api/alimentos.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/alimentos",
      "dest": "/pages/api/alimentos.js"
    }
  ],
    "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*" // Permite todas as origens. Substitua por uma URL específica se necessário.
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,POST,PUT,DELETE,OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
}
